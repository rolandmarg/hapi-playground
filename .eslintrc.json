{
  "env": {
    "commonjs": true,
    "jest": true
  },
  "plugins": ["prettier", "jest"],
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      { "devDependencies": ["**/*test.js"] }
    ],
    "no-console": [
      2,
      {
        "allow": ["warn", "error"]
      }
    ]
  }
}
